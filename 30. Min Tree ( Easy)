# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def minDepth(self, root: TreeNode) -> int:
        if root is None:
            return 0
        count = 1
        return min(self.get_min(root.left,count), self.get_min(root.right, count))
    def get_min(self, root,count):
        if root is None:
            return count
        left_root, right_root = self.get_min(root.left,count), self.get_min(root.right, count)
        if left_root is None and right_root is None:
            return count
        count += 1
        return count
        
        
 solution 2 success
 
# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def minDepth(self, root: TreeNode) -> int:
        if root is None:
            return 0
        if root.left and root.right:
            return min(self.minDepth(root.left),self.minDepth(root.right)) +1
        else:
            return max(self.minDepth(root.left),self.minDepth(root.right)) +1
        
